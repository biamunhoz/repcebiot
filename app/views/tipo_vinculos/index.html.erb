<p id="notice"><%= notice %></p>

<h1>Tipo Vinculos</h1>

<table>
  <thead>
    <tr>
      <th>Tipovinculo</th>
      <th>Codigosetor</th>
      <th>Nomeabreviadsetor</th>
      <th>Nomesetor</th>
      <th>Codigounidade</th>
      <th>Siglaunidade</th>
      <th>Nomeunidade</th>
      <th>Nomevinculo</th>
      <th>Nomeabreviadofuncao</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_vinculos.each do |tipo_vinculo| %>
      <tr>
        <td><%= tipo_vinculo.tipoVinculo %></td>
        <td><%= tipo_vinculo.codigoSetor %></td>
        <td><%= tipo_vinculo.nomeAbreviadSetor %></td>
        <td><%= tipo_vinculo.nomeSetor %></td>
        <td><%= tipo_vinculo.codigoUnidade %></td>
        <td><%= tipo_vinculo.siglaUnidade %></td>
        <td><%= tipo_vinculo.nomeUnidade %></td>
        <td><%= tipo_vinculo.nomeVinculo %></td>
        <td><%= tipo_vinculo.nomeAbreviadoFuncao %></td>
        <td><%= tipo_vinculo.usuario %></td>
        <td><%= link_to 'Show', tipo_vinculo %></td>
        <td><%= link_to 'Edit', edit_tipo_vinculo_path(tipo_vinculo) %></td>
        <td><%= link_to 'Destroy', tipo_vinculo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Vinculo', new_tipo_vinculo_path %>
